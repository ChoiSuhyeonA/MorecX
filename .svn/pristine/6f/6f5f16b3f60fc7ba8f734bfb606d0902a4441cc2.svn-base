package kr.co.irlink.zirecx.system.service;

import java.io.Serializable;


public class GroupListVO implements Serializable {


	/** UID */
	private static final long serialVersionUID = 6458609047525797490L;

	/** 전체조직 */
    private String groupPath;

	/** 전체Depth */
    private String groupDepth;
    
    /** 조직명 */
    private String groupName;
    
    /** 조직코드 */
    private String groupSeq;
    
    /** 부모조직코드 */
    private String parentGroupSeq;
   

	/** 삭제여부 */
    private String deleteYn;
    
    private int licenseCnt;
    
    
    
    
  	public int getLicenseCnt() {
		return licenseCnt;
	}

	public void setLicenseCnt(int licenseCnt) {
		this.licenseCnt = licenseCnt;
	}

	public String getGroupPath() {
  		return groupPath;
  	}

  	public void setGroupPath(String groupPath) {
  		this.groupPath = groupPath;
  	}
  	 
    public String getParentGroupSeq() {
		return parentGroupSeq;
	}

	public void setParentGroupSeq(String parentGroupSeq) {
		this.parentGroupSeq = parentGroupSeq;
	}
	
    public String getGroupDepth() {
		return groupDepth;
	}

	public void setGroupDepth(String groupDepth) {
		this.groupDepth = groupDepth;
	}

	public String getGroupName() {
		return groupName;
	}

	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}

	public String getGroupSeq() {
		return groupSeq;
	}

	public void setGroupSeq(String groupSeq) {
		this.groupSeq = groupSeq;
	}

	public String getDeleteYn() {
		return deleteYn;
	}

	public void setDeleteYn(String deleteYn) {
		this.deleteYn = deleteYn;
	}
}
